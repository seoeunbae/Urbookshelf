<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidstudio.practiceurbook">
    <uses-permission android:name="android.permission.INTERNET" />                                  //인터넷커미션
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PracticeUrbook"
        android:name="androidx.multidex.MultiDexApplication" >>

        <activity android:name=".MainActivity">
            <intent-filter>                                                                         //시작엑티비티정해주는부분
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">                                                    //생성한 자바파일들을 매니페스트 등록해주기
        </activity>
        <activity android:name=".MemberinitActivity">
        </activity>
        <activity android:name=".SignUpActivity">
        </activity>
        <activity android:name=".PasswordResetActivity">
        </activity>

    </application>

</manifest>